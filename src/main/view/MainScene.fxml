<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.view.MainSceneController">
   <children>
      <ScrollPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <content>
            <VBox prefHeight="850.0" prefWidth="1300.0">
               <children>
                  <MenuBar>
                     <menus>
                        <Menu mnemonicParsing="false" text="File">
                           <items>
                              <MenuItem mnemonicParsing="false" onAction="#open" text="Open" />
                              <MenuItem mnemonicParsing="false" onAction="#close" text="Close" />
                           </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Book">
                           <items>
                              <MenuItem mnemonicParsing="false" onAction="#next" text="next" />
                              <MenuItem mnemonicParsing="false" onAction="#previous" text="previous" />
                              <MenuItem mnemonicParsing="false" onAction="#clear" text="clear" />
                              <MenuItem mnemonicParsing="false" onAction="#remove" text="remove" />
                           </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Quickdraw">
                           <items>
                              <MenuItem mnemonicParsing="false" onAction="#quickDraw" text="set1" />
                           </items>
                        </Menu>
                        <Menu fx:id="colorMenu" mnemonicParsing="false" text="Color" />
                        <Menu fx:id="menu" mnemonicParsing="false" text="Page: " />
                     </menus>
                  </MenuBar>
                  <AnchorPane fx:id="pane" minHeight="0.0" minWidth="0.0" prefHeight="500.0" prefWidth="500.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </AnchorPane>
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
